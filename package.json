{
  "name": "strawberry-graphql",
  "version": "1.0.0",
  "author": "Patrick Arminio <patrick.arminio@@gmail.com>",
  "engines": {
    "node": "16.x",
    "npm": "7.x"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "prebuild": "npm run codegen",
    "build": "next build",
    "develop": "npm run dev",
    "dev": "next dev",
    "dev:inspect": "NODE_OPTIONS='--inspect' next dev",
    "start": "next start",
    "format": "prettier --write \"{,!(.next|node_modules|public|coverage|.cache)/**/}*.{js,jsx,ts,tsx,json}\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --watch --noEmit",
    "codegen": "graphql-codegen --config codegen.yml",
    "codegen:watch": "npm run codegen -- --watch",
    "check-updates": "npm-check -u",
    "lint": "next lint",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "keywords": [
    "strawberry-graphql",
    "strawberry",
    "graphql",
    "next.js",
    "typescript"
  ],
  "dependencies": {
    "@docsearch/js": "3.2.0",
    "@docsearch/react": "3.2.0",
    "@hashicorp/remark-plugins": "4.0.0",
    "@heroicons/react": "1.0.6",
    "@octokit/core": "4.0.4",
    "@octokit/plugin-paginate-rest": "3.1.0",
    "@tailwindcss/aspect-ratio": "0.4.0",
    "classnames": "2.3.1",
    "github-slugger": "1.4.0",
    "gray-matter": "4.0.3",
    "hast-util-to-html": "8.0.3",
    "hast-util-to-string": "2.0.0",
    "hastscript": "7.0.2",
    "jsonp": "0.2.1",
    "marked": "4.0.18",
    "mdast-util-heading-range": "3.1.0",
    "next": "12.2.4",
    "next-mdx-remote": "4.1.0",
    "npm-check": "6.0.1",
    "nprogress": "0.2.0",
    "parse-numeric-range": "1.3.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-instantsearch-dom": "6.31.1",
    "react-refractor": "2.1.7",
    "refractor": "4.7.0",
    "rehype-parse": "8.0.4",
    "to-querystring": "1.1.1",
    "typescript": "4.7.4",
    "unified": "10.1.2",
