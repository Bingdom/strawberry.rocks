    "@storybook/manager-webpack4": "6.5.10",
    "@storybook/react": "6.5.10",
    "@storybook/testing-library": "0.0.13",
    "@trivago/prettier-plugin-sort-imports": "3.3.0",
    "@types/github-slugger": "1.3.0",
    "@types/jsonp": "0.2.1",
    "@types/marked": "4.0.3",
    "@types/node": "18.6.5",
